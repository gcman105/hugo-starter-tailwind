{
  "name": "hugo-starter-tailwind",
  "version": "0.1.0",
  "description": "Tailwindcss starter theme for Hugo",
  "scripts": {
    "dev": "NODE_ENV=development ./node_modules/tailwindcss/lib/cli.js -i ./assets/css/main.css -o ./assets/css/compiled/main.css --jit -w",
    "build": "NODE_ENV=production ./node_modules/tailwindcss/lib/cli.js -i ./assets/css/main.css -o ./assets/css/compiled/main.css --jit",
    "example": "hugo server --source exampleSite --themesDir ../.. --buildDrafts -b http://localhost/example/ -p 8008"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gcman105/hugo-starter-tailwind.git"
  },
  "keywords": [
    "hugo",
    "hugo-theme",
    "tailwind",
    "dark-mode"
  ],
  "author": "Gary Cheeseman",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gcman105/hugo-starter-tailwind/issues"
  },
  "homepage": "https://github.com/gcman105/hugo-starter-tailwind#readme",
  "devDependencies": {
    "@tailwindcss/typography": "^0.4.1",
    "autoprefixer": "^10.4.0",
    "postcss": "^8.4.4",
    "prettier": "^2.4.1",
    "prettier-plugin-go-template": "^0.0.11",
    "tailwindcss": "^2.2.19"
  }
}
